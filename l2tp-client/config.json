{
  "name": "CHCloudClient",
  "version": "0.7.24",
  "slug": "l2tp",
  "description": "CH Cloud Client",
  "startup": "system",
  "boot": "auto",
  "map": ["config"],
  "host_network": true,
  "full_access": false,
  "ingress": true,
  "ingress_port": 8099,
  "hassio_api": false,
  "hassio_role": "manager",
  "auth_api": false,
  "homeassistant_api": true,
  "auto_uart": false,
  "gpio": false,
  "apparmor": false,
  "host_dbus": false,
  "stdin": false,
  "docker_api": false,
  "panel_icon": "mdi:virtual-private-network",
  "devices": [ 
	  "/dev/ppp:/dev/ppp:rwm",
	  "/dev/mem:/dev/mem:rwm"
  ],
  "privileged" : [ 
  	"NET_ADMIN"
  ],
  "options": {
    "server": "chcloud.home.bi.group",
    "login": "162@chcloud.home.bi.group",
    "password": "smarthome",
    "salt": 3
  },
  "schema": {
    "server": "str",
    "login": "email",
    "password": "str",
    "salt": "int(0,16)"
  },
  "environment" : {
    "L2TP_SERVER_IP": "10.64.0.1",
    "LOGIN": "`$hostname`"
  },
  "arch": [
    "amd64",
    "armhf",
    "i386",
    "armv7"
  ],
  "image" : "oilab/{arch}-chcloud-client"
}
